<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email ID"
        android:id="@+id/textView_emailID"
        android:layout_marginLeft="43dp"
        android:layout_marginStart="43dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_emailID"
        android:layout_alignTop="@+id/textView_emailID"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        android:id="@+id/textView_name"
        android:layout_below="@+id/textView_emailID"
        android:layout_alignLeft="@+id/textView_emailID"
        android:layout_alignStart="@+id/textView_emailID"
        android:layout_marginTop="13dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_name"
        android:layout_alignBottom="@+id/textView_name"
        android:layout_alignRight="@+id/text_emailID"
        android:layout_alignEnd="@+id/text_emailID" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Course"
        android:id="@+id/textView_course"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/textView_name"
        android:layout_alignLeft="@+id/textView_name"
        android:layout_alignStart="@+id/textView_name" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_course"
        android:layout_alignTop="@+id/textView_course"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Branch"
        android:id="@+id/textView_branch"
        android:layout_below="@+id/textView_course"
        android:layout_alignLeft="@+id/textView_course"
        android:layout_alignStart="@+id/textView_course"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_branch"
        android:layout_alignTop="@+id/textView_branch"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Institution"
        android:id="@+id/textView_institution"
        android:layout_below="@+id/textView_branch"
        android:layout_alignLeft="@+id/textView_branch"
        android:layout_alignStart="@+id/textView_branch"
        android:layout_marginTop="17dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_institution"
        android:layout_alignTop="@+id/textView_institution"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Skills"
        android:id="@+id/textView_skill"
        android:layout_below="@+id/textView_institution"
        android:layout_alignLeft="@+id/textView_institution"
        android:layout_alignStart="@+id/textView_institution"
        android:layout_marginTop="19dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_skills"
        android:layout_alignTop="@+id/textView_skill"
        android:layout_alignRight="@+id/text_institution"
        android:layout_alignEnd="@+id/text_institution" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rating"
        android:id="@+id/textView_rating"
        android:layout_below="@+id/textView_skill"
        android:layout_alignRight="@+id/textView_branch"
        android:layout_alignEnd="@+id/textView_branch"
        android:layout_marginTop="83dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_rating"
        android:layout_alignTop="@+id/textView_rating"
        android:layout_alignRight="@+id/text_skills"
        android:layout_alignEnd="@+id/text_skills" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        android:id="@+id/textView_gender"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/textView_rating"
        android:layout_alignLeft="@+id/textView_rating"
        android:layout_alignStart="@+id/textView_rating" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_gender"
        android:layout_alignTop="@+id/textView_gender"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DOB"
        android:id="@+id/textView_dob"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/textView_gender"
        android:layout_alignLeft="@+id/textView_gender"
        android:layout_alignStart="@+id/textView_gender" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_dob"
        android:layout_alignTop="@+id/textView_dob"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Edit Profile"
        android:id="@+id/button_editprofile"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/textView_dob"
        android:layout_alignLeft="@+id/textView_dob"
        android:layout_alignStart="@+id/textView_dob" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proceed"
        android:id="@+id/button_proceed"
        android:layout_alignTop="@+id/button_editprofile"
        android:layout_toRightOf="@+id/text_dob"
        android:layout_toEndOf="@+id/text_dob"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp" />
</RelativeLayout>
</ScrollView>